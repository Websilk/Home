{
  "title": "Websilk CMS Platform",
  "version": "1.0",
  "authors": [ "Mark Entingh" ],
  "description": "Websilk is a powerful drag & drop web development CMS platform built for Windows, Linux, & Mac OSX",
  "packOptions": {
    "projectUrl": "http://www.github.com/Websilk/Home",
    "owners": [ "Mark Entingh" ]
  },

  "commands": {},

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-*",
          "type": "platform"
        },
        "Microsoft.AspNetCore.Hosting": "1.0.0-*",
        "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*",
        "Microsoft.AspNetCore.StaticFiles": "1.0.0-*",
        "Microsoft.AspNetCore.Diagnostics": "1.0.0-*",
        "Microsoft.Extensions.Caching.Memory": "1.0.0-*",
        "Microsoft.AspNetCore.Session": "1.0.0-*",
        "Microsoft.Extensions.Configuration": "1.0.0-*",
        "Microsoft.Extensions.Configuration.Json": "1.0.0-*",
        "System.Text.Encoding": "4.3.0-*",
        "System.Text.Encoding.CodePages": "4.3.0-*",
        "System.Text.Encodings.Web": "4.3.0-*",
        "System.Text.RegularExpressions": "4.3.0-*",
        "System.ComponentModel": "4.3.0-*",
        "System.Console": "4.3.0-*",
        "System.Runtime": "4.3.0-*",
        "System.Runtime.Extensions": "4.3.0-*",
        "System.Runtime.InteropServices": "4.3.0-*",
        "System.Runtime.Serialization.Xml": "4.3.0-*",
        "System.Collections": "4.3.0-*",
        "System.Linq": "4.3.0-*",
        "System.IO": "4.3.0-*",
        "System.Data.Common": "4.3.0-*",
        "System.Data.SqlClient": "4.3.0-*",
        "System.Diagnostics.Tools": "4.3.0-*",
        "System.Diagnostics.Process": "4.3.0-*",
        "System.Xml.ReaderWriter": "4.3.0-*",
        "System.Xml.XmlDocument": "4.3.0-*",
        "System.Xml.XmlSerializer": "4.3.0-*",
        "System.Resources.ResourceManager": "4.3.0-*",
        "System.ServiceModel.Security": "4.3.0-*",
        "System.Threading": "4.3.0-*",
        "System.Threading.Tasks": "4.3.0-*",
        "System.Net.Http": "4.3.0-*",
        "System.Xml.XPath.XmlDocument": "4.3.0-*",
        "Newtonsoft.Json": "9.0.1",
        "BCrypt.Net-Core": "1.1.0"
      }
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "compile": [
      "*.cs",
      "App/**/*.cs"
    ],
    "compileExclude": [
      "wwwroot",
      "Sql"
    ],
    "publishExclude": [
      "node_modules",
      "bower_components",
      "**.xproj",
      "**.user",
      "**.vspscc",
      "Sql"
    ],
    "exclude": [
      "node_modules",
      "bower_components"
    ],
    "optimize": true
  }
}
